var name="KGF";
var rating=4.5;
var type="action";
var ind="sandalwood";

var out="An " +name+" is an "+type+" movie with rating of "+rating+" and from ind " +ind+    "."

out
'An KGF is an action movie with rating of 4.5 and from ind sandalwood.'//es5


var name="KGF";
var rating=4.5;
var type="action";
var ind="sandalwood";

var out=`An  ${name} is an ${type} movie with rating of ${rating} and from ind  ${ind}    .`

out
'An  KGF is an action movie with rating of 4.5 and from ind  sandalwood    .'


